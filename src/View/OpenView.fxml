<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane fx:id="border" stylesheets="@OpenViewStyle.css" prefHeight="298.0" prefWidth="419.0" maxHeight="NaN" minWidth="-NaN" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.OpenViewController">
   <center>
      <VBox fx:id="vbox" prefHeight="200.0" prefWidth="100.0" spacing="2.0" BorderPane.alignment="CENTER">

         <children>
            <AnchorPane fx:id="mediaViewPane">
               <MediaView fx:id="media" fitHeight="${border.maxHeight}"  fitWidth="${border.maxWidth}"   />
               <Button fx:id="startBotton" alignment="CENTER" contentDisplay="CENTER"  mnemonicParsing="false" onAction="#stopVideo" prefHeight="41.0" prefWidth="72.0" text="start" GridPane.rowIndex="2" />

            </AnchorPane>
         </children>
      </VBox>
   </center>

</BorderPane>

